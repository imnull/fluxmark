{
  "name": "@streaming-markdown/angular",
  "version": "0.1.0",
  "description": "Angular components for streaming markdown renderer",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.css"
    }
  },
  "scripts": {
    "build": "ngc && cp src/styles.css dist/styles.css",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "markdown",
    "angular",
    "streaming",
    "ai",
    "chat"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": ">=17.0.0",
    "@angular/common": ">=17.0.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "@streaming-markdown/core": "file:../core"
  },
  "devDependencies": {
    "@angular/compiler": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "rxjs": "^7.8.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
